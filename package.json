    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "0.33.0",
    "eslint": "^8.43.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "4.1.5",
    "prisma": "^5.0.0",
    "supertest": "6.3.3",
    "tsup": "7.0.0",
    "tsx": "3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  }
